datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Room {
    id String @id @default(cuid())
    name String
    usersToRooms UserToRoom[]
}

model UserToRoom {
    id String @id @default(cuid())
    userId String
    roomId String
    room Room @relation(fields: [roomId], references: [id])
}